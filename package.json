{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "staging": "vite dist --host 0.0.0.0",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint -- --fix",
    "lint:pr": "eslint src/**/*.{ts,tsx} --fix-dry-run --max-warnings 0",
    "prettier": "prettier --write \"**/*.{ts,tsx,json,scss,md}\"",
    "generate": "graphql-codegen --config codegen/codegen.yaml",
    "generate:staging": "graphql-codegen --config codegen/codegen-staging.yaml",
    "generate:local": "graphql-codegen --config codegen/codegen-local.yaml"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typed-document-node": "^2.2.10",
    "@graphql-codegen/typescript": "^2.4.10",
    "@graphql-codegen/typescript-operations": "^2.3.7",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.31",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.0",
    "clsx": "^1.1.1",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.4",
    "graphql": "^16.4.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-hook-form": "^7.33.0",
    "react-router-dom": "^6.3.0",
    "recharts": "^2.1.10",
    "subscriptions-transport-ws": "^0.11.0",
    "typescript": "^4.6.3",
    "urql": "^2.2.0"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/react-helmet": "^6.1.5",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.14.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "jsdom": "^19.0.0",
    "postcss": "^8.4.13",
    "prettier": "2.6.2",
    "tailwindcss": "^3.0.24",
    "vite": "^2.9.9",
    "vitest": "^0.13.0"
  }
